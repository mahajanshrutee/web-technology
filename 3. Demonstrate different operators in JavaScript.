<html>
<body>
<script type="text/javascript">
var a=10;
var b=20;
var c="test";
var linebreak = "<br/>";
document.write("a+b=")

result = a+b;
document.write(result);
document.write(linebreak);
document.write("a-b =");

result = (a-b);
document.write(result);
document.write(linebreak);
document.write("a / b = ");

result = (a/b);
document.write(result);
document.write(linebreak);
document.write("a % b = ");

result = a % b;
document.write(result);
document.write(linebreak);
document.write("a + b + c = ");

result = a+b+c;
document.write(result);
document.write(linebreak);
a++; 
document.write("a++ = ");
document.write(a++);
document.write(linebreak);
b--;
document.write("b-- =");
document.write(b--);
document.write(linebreak);
</script>
</body>
</html>
